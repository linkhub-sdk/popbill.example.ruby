<div id="content">
  <p class="heading1">Response</p>
  <br/>
  <fieldset class="fieldset1">
    <legend><%= request.env['PATH_INFO'] %></legend>
    <fieldset class="fieldset2">
      <legend>휴폐업조회 결과확인 - 단건</legend>
      <ul>
        <li>corpNum(사업자번호) : <%= @Response["corpNum"] %></li>
        <li>type(사업자 과세유형) : <%= @Response["type"] %></li>
        <li>state(휴폐업상태) : <%= @Response["state"] %></li>
        <li>stateDate(휴폐업일자) : <%= @Response["stateDate"] %></li>
        <li>typeDate(과세유형 전환일자) : <%= @Response["typeDate"] %></li>
        <li>checkDate(확인일자) : <%= @Response["checkDate"] %></li>
      </ul>
    </fieldset>
  </fieldset>
</div>
