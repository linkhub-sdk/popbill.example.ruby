<div id="content">
  <p class="heading1">Response</p>
  <br/>
  <fieldset class="fieldset1">
    <legend><%= request.env['PATH_INFO'] %></legend>
    <fieldset class="fieldset2">
      <legend>계좌실명조회</legend>
      <ul>
        <li>bankCode (기관코드) : <%= @Response["bankCode"] %></li>
        <li>accountNumber (계좌번호) : <%= @Response["accountNumber"] %></li>
        <li>accountName (예금주 성명) : <%= @Response["accountName"] %></li>
        <li>checkDate (확인일시) : <%= @Response["checkDate"] %></li>
        <li>identityNumType (등록번호 유형) : <%= @Response["identityNumType"] %></li>
        <li>identityNum (등록번호) : <%= @Response["identityNum"] %></li>
        <li>result (응답코드) : <%= @Response["result"] %></li>
        <li>resultMessage (응답메시지) : <%= @Response["resultMessage"] %></li>
      </ul>
    </fieldset>
  </fieldset>
</div>
