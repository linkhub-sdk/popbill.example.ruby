<div id="content">
  <p class="heading1">Response</p>
  <br/>
  <fieldset class="fieldset1">
    <legend><%= request.env['PATH_INFO'] %></legend>
    <% @Response.each_with_index do |statementLog, index| %>
      <fieldset class="fieldset2">
        <legend>전자명세서 문서상태 변경이력 [<%= (index + 1) %>/<%= @Response.length %>]</legend>
        <ul>
          <li>docLogType(로그타입) : <%= statementLog["docLogType"] %></li>
          <li>log(이력정보) : <%= statementLog["log"] %></li>
          <li>procType(처리형태) : <%= statementLog["procType"] %></li>
          <li>procCorpName(처리회사명) : <%= statementLog["procCorpName"] %></li>
          <li>procContactName(처리담당자명) : <%= statementLog["procContactName"] %></li>
          <li>procMemo(처리메모) : <%= statementLog["procMemo"] %></li>
          <li>regDT(등록일시) : <%= statementLog["regDT"] %></li>
          <li>ip(아이피) : <%= statementLog["ip"] %></li>
        </ul>
      </fieldset>
    <% end %>
  </fieldset>
</div>
