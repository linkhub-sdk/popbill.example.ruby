<div id="content">
  <p class="heading1">Response</p>
  <br/>
  <fieldset class="fieldset1">
    <legend><%= request.env['PATH_INFO'] %></legend>
    <ul>
      <li>itemCode (문서종류코드) : <%= @Response["itemCode"] %></li>
      <li>mgtKey (문서번호) : <%= @Response["mgtKey"] %></li>
      <li>invoiceNum (문서고유번호) : <%= @Response["invoiceNum"] %></li>
      <li>formCode (맞춤양식 코드) : <%= @Response["formCode"] %></li>
      <li>writeDate (작성일자) : <%= @Response["writeDate"] %></li>
      <li>taxType (세금형태) : <%= @Response["taxType"] %></li>
      <li>purposeType (영수/청구) : <%= @Response["purposeType"] %></li>
      <li>serialNum (일련번호) : <%= @Response["serialNum"] %></li>
      <li>taxTotal (세액 합계) : <%= @Response["taxTotal"] %></li>
      <li>supplyCostTotal (공급가액 합계) : <%= @Response["supplyCostTotal"] %></li>
      <li>totalAmount (합계금액) : <%= @Response["totalAmount"] %></li>
      <li>remark1 (비고1) : <%= @Response["remark1"] %></li>
      <li>remark2 (비고2) : <%= @Response["remark2"] %></li>
      <li>remark3 (비고3) : <%= @Response["remark3"] %></li>

      <li>senderCorpNum (발신자 사업자번호) : <%= @Response["senderCorpNum"] %></li>
      <li>senderTaxRegID (발신자 종사업장번호) : <%= @Response["senderTaxRegID"] %></li>
      <li>senderCorpName (발신자 상호) : <%= @Response["senderCorpName"] %></li>
      <li>senderCEOName (발신자 대표자 성명) : <%= @Response["senderCEOName"] %></li>
      <li>senderAddr (발신자 주소) : <%= @Response["senderAddr"] %></li>
      <li>senderBizType (발신자 업태) : <%= @Response["senderBizType"] %></li>
      <li>senderBizClass (발신자 종목) : <%= @Response["senderBizClass"] %></li>
      <li>senderContactName (발신자 성명) : <%= @Response["senderContactName"] %></li>
      <li>senderTEL (발신자 연락처) : <%= @Response["senderTEL"] %></li>
      <li>senderHP (발신자 휴대전화) : <%= @Response["senderHP"] %></li>
      <li>senderEmail (발신자 이메일주소) : <%= @Response["senderEmail"] %></li>
      <li>senderFAX (발신자 팩스번호) : <%= @Response["senderFAX"] %></li>

      <li>receiverCorpNum (수신자 사업자번호) : <%= @Response["receiverCorpNum"] %></li>
      <li>receiverTaxRegID (수신자 종사업장번호) : <%= @Response["receiverTaxRegID"] %></li>
      <li>receiverCorpName (수신자 상호) : <%= @Response["receiverCorpName"] %></li>
      <li>receiverCEOName (수신자 대표자 성명) : <%= @Response["receiverCEOName"] %></li>
      <li>receiverAddr (수신자 주소) : <%= @Response["receiverAddr"] %></li>
      <li>receiverBizType (수신자 업태) : <%= @Response["receiverBizType"] %></li>
      <li>receiverBizClass (수신자 종목) : <%= @Response["receiverBizClass"] %></li>
      <li>receiverContactName (수신자 성명) : <%= @Response["receiverContactName"] %></li>
      <li>receiverTEL (수신자 연락처) : <%= @Response["receiverTEL"] %></li>
      <li>receiverHP (수신자 휴대전화) : <%= @Response["receiverHP"] %></li>
      <li>receiverEmail (수신자 이메일주소) : <%= @Response["receiverEmail"] %></li>
      <li>receiverFAX (수신자 팩스번호) : <%= @Response["receiverFAX"] %></li>

      <% @Response["detailList"].each_with_index do |detailInfo, index| %>
        <fieldset class="fieldset2">
          <legend>전자명세서 상세항목 [<%= (index + 1) %>/<%= @Response["detailList"].length %>]</legend>
          <ul>
            <li>serialNum (일련번호) : <%= detailInfo["serialNum"] %></li>
            <li>purchaseDT (거래일자) : <%= detailInfo["purchaseDT"] %></li>
            <li>itemName (품명) : <%= detailInfo["itemName"] %></li>
            <li>spec (규격) : <%= detailInfo["spec"] %></li>
            <li>qty (수량) : <%= detailInfo["qty"] %></li>
            <li>unitCost (단가) : <%= detailInfo["unitCost"] %></li>
            <li>supplyCost (공급가액) : <%= detailInfo["supplyCost"] %></li>
            <li>tax (세액) : <%= detailInfo["tax"] %></li>
            <li>remark (비고) : <%= detailInfo["remark"] %></li>
          </ul>
        </fieldset>
      <% end %>

    </ul>
  </fieldset>
</div>
