<div id="content">
  <p class="heading1">Response</p>
  <br/>
  <fieldset class="fieldset1">
    <legend><%= request.env['PATH_INFO'] %></legend>
    <% @Response.each_with_index do |emailInfo, index| %>
      <fieldset class="fieldset2">
        <legend>GetEmailPublicKeys [<%= (index + 1) %>/<%= @Response.length %>]</legend>
        <ul>
          <li>confirmNum : <%= emailInfo["confirmNum"] %></li>
          <li>email : <%= emailInfo["email"] %></li>
        </ul>
      </fieldset>
    <% end %>
  </fieldset>
</div>
