<div id="content">
  <p class="heading1">Response</p>
  <br/>
  <fieldset class="fieldset1">
    <legend><%=request.env['PATH_INFO']%></legend>
    <% @Response.each_with_index do |emailInfo, index| %>
    <fieldset class="fieldset2">
      <legend>유통사업자 메일 목록 확인 [<%= (index+1)%>/<%=@Response.length%>]</legend>
      <ul>
        <li>confirmNum (유통사업자 승인번호) : <%=emailInfo["confirmNum"]%></li>
        <li>email (메일) : <%=emailInfo["email"]%></li>
      </ul>
    </fieldset>
    <%end%>
  </fieldset>
 </div>
